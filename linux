//to enable service
  systemctl enable ServiceName.service
//to start service now
  systemctl start ServiceName.service 
//to check status
  systemctl status ServiceName.service
 
 Services
   NetworkManager.service (need network manager package)
   dhcpcd.service


run - below to setup virtual box

sudo pacman -S virtualbox-guest-modules-arch virtualbox-guest-utils

pacman -S xorg virtualbox-guest-utils

sudo pacman -S xorg-server xorg-apps xorg-xinit

sudo pacman -S noto-fonts

sudo pacman -S i3-gaps i3status rxvt-unicode dmenu

